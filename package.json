{
	"name": "twig-challenge",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=20.x",
		"npm": ">=9.x"
	},
	"scripts": {
		"build": "tsc",
		"build:esbuild": "etsc --config etsc.config.js",
		"build:cached": "turbo build",
		"build:cached:esbuild": "turbo build:esbuild",
		"start": "node --trace-warnings --trace-deprecation --experimental-modules --es-module-specifier-resolution=node dist",
		"dev": "tsc-watch --onSuccess 'node --trace-warnings --trace-deprecation --experimental-modules --es-module-specifier-resolution=node dist'",
		"dev:esbuild": "nodemon",
		"lint": "eslint --ext .ts,.js . --ignore-path .gitignore",
		"fix": "eslint --ext .ts,.js . --ignore-path .gitignore --fix",
		"lint-staged": "lint-staged",
		"test": "vitest dev",
		"test:unit": "vitest run -t plugins?",
		"test:ci": "vitest run",
		"test:coverage": "vitest run --coverage",
		"push": "prisma db push --skip-generate",
		"pull": "prisma db pull",
		"seed": "prisma db seed",
		"seed:dev": "CREATE_ADMIN_CREDENTIALS=true prisma db seed",
		"generate": "prisma generate",
		"postinstall": "husky install && prisma generate"
	},
	"keywords": [],
	"author": "phr3nzy <adilosama47@gmail.com>",
	"license": "SEE LICENSE.md",
	"devDependencies": {
		"@types/amqplib": "^0.10.1",
		"@types/node": "^20.4.1",
		"@types/qs": "^6.9.7",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-c8": "^0.33.0",
		"cpy": "^10.1.0",
		"esbuild": "^0.18.11",
		"esbuild-node-tsc": "^2.0.5",
		"eslint": "^8.44.0",
		"eslint-config-async": "^2.0.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-security": "^1.7.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.0",
		"prisma": "^5.0.0",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.1.6",
		"vitest": "^0.33.0"
	},
	"dependencies": {
		"@fastify/cors": "^8.3.0",
		"@fastify/env": "^4.2.0",
		"@fastify/sensible": "^5.2.0",
		"@fastify/swagger": "^8.8.0",
		"@fastify/swagger-ui": "^1.9.2",
		"@fastify/under-pressure": "^8.3.0",
		"@prisma/client": "^5.0.0",
		"@total-typescript/ts-reset": "^0.4.2",
		"ajv": "^8.12.0",
		"ajv-errors": "^3.0.0",
		"ajv-formats": "^2.1.1",
		"amqplib": "^0.10.3",
		"axios": "^1.4.0",
		"fastify": "^4.19.2",
		"fastify-plugin": "^4.5.0",
		"fluent-json-schema": "^4.1.0",
		"magic-regexp": "^0.7.0",
		"make-promises-safe": "^5.1.0",
		"pino": "^8.14.1",
		"pino-pretty": "^10.0.1",
		"qs": "^6.11.2",
		"sleep-promise": "^9.1.0",
		"turbo": "^1.10.7",
		"uuid": "^9.0.0"
	},
	"lint-staged": {
		"*.{js,ts,json,md,yml,yaml}": [
			"prettier --write"
		]
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}
